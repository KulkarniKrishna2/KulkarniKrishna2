		},
		"createClient": {
			"isMandatoryField": {
				"dateOfBirth": true,
				"mobileNumber": true,
				"addressType": true
			},
			"isValidateDOBField": {
				"active": true,
				"ageCriteria": {
					"minAge": "18",
					"maxAge": "60"
				}
			}
		},
		"createCenter": {
			"isReadOnlyField": {
				"active": true
			}
		},
		"createLoanApplication": {
			"isHiddenField": {
				"firstRepaymentDate": false
			},
			"newLoanApplicationLimitAllowed" : true
		},
		"loanAccount": {
			"isHiddenField": {
				"repaymentsStartingFromDate": false
			}
		}
	}
}