{
  "enableUIDisplayConfiguration": true,
  "uiDisplayConfigurations": {
    "loanAccount": {
      "isHiddenField": {
        "interestChargedFromDate": true
      },
      "isAutoPopulate": {
        "interestChargedFromDate": true
      }
    },
    "createCenter": {
      "isReadOnlyField": {
        "active": true,
        "name": true
      },
      "isHiddenField": {
        "villageOptions": false
      }
    },
    "workflow": {
      "loanApproval": {
        "hiddenField": {
          "clientAddress": false
        }
      },
      "isMandatory": {
        "rejectReason":true,
        "rejectDescription": true
      }
    },
    "collectionSheet": {
      "isHiddenFeild": {
        "paymentDetails": true

      }
    },
    "createClient": {
      "isReadOnlyField": {
        "gender": true
      },
      "isValidateDOBField": {
        "active": true,
        "ageCriteria": {
          "minAge": 18,
          "maxAge": 60
        }
      },
      "isMandatoryField": {
        "dateOfBirth": true,
        "mobileNumber":true,
      },
      "defaultGender":"Female"
    },
    "bankAccountDetails": {
      "isMandatory": {
      "bankAccountHolderName": true,
      "bankAccountNumber": true,
      "accountType": false,
      "bankIFSCCode": true,
      "bankName": false,
      "bankCity": false,
      "mobileNumber": false,
      "lastTransactionDate": false,
      "docFile": false
      },
      "isHiddenField":{
        "bankIFSCCodeRepeat":false,
        "bankAccountNumberRepeat":false,
      "lastTransactionDate": true,
        "micrCode": true
      },
      "regexValidations" :{
        "mobileNumber":"^\\d{10}$",
        "bankIFSCCode":"^[A-Za-z]{4}\\d{7}$",
        "bankCity":"^[A-Za-z]{1,20}$",
        "bankName":"^(?!.*?\\s{2})[A-Za-z ]{1,20}$",
        "bankAccountNumber":"^[0-9]{1,50}$",
        "bankAccountHolderName":"^[a-zA-Z]+([a-zA-Z\\s][a-zA-Z]+)*$"
      }
    },
    "viewGroup":{
      "searchForActivegroups": false
    },
    "createLoanApplication": {
      "newLoanApplicationLimitAllowed" : true
    }
  }
}