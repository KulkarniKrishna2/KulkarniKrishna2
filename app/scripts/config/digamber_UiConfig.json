		"viewCenter": {
			"isHiddenField": {
				"accountNo": true,
				"referenceNo" : false
			}
		},
		"clientIdentifier": {
			"hiddenFields": {
				"documentIssueDate": true,
				"documentExpiryDate": true
			}
		},
		"editCalendar": {
			"editableFields": {
				"repeat": false,
				"repeatsevery": false,
				"repeatson": false,
				"meetingStartDate":true
			}
