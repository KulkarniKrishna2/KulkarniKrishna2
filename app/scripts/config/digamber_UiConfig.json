			"isMandatory": {
				"fundId": true
			},
			"hiddenField": {
				"syncDisbursementWithMeeting": true
			}
		},
		"bankAccountDetails": {
			"isMandatory": {
				"bankAccountHolderName": true,
				"bankAccountNumber": true,
